{
    "name": "关键词屏蔽",
    "manifest_version": 2,
    "version": "1.0",
    "description": "关键词屏蔽",

    "browser_action":
    {
        "default_title": "关键词屏蔽",
        "default_popup": "popup.html"
    },
    
    "icons":{
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["filter.js"]
    }]
}